# variable cc will be the compiler to use
CC=gcc

# the .c files to be executed
SRC=main.c school.c

#the name of the executable
NAME=school

#the .o object files
OBJ=*.o

all:school

school: main.c school.c
	$(CC) $(SRC) $(OBJ) -o school


main.o: main.c
	gcc -c main.c -o main.o

school.o: school.c
	gcc -c school.c -o schoo.o
