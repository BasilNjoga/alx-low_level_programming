CC=gcc
SRC=*.c
OBJ=*.o
NAME= school

all: $(NAME)

$(NAME): $(OBJ)
	$(CC) $(OBJ) -o $(NAME)

clean:
	rm -f *.o $(NAME)
oclean:
	rm -f *.o                                                 
fclean:
	rm -f school *.o                                          

re: school                                                        

~            
